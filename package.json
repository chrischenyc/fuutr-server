    "bcrypt": "^3.0.2",
    "bluebird": "^3.5.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "debug": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "express-winston": "^3.0.1",
    "handlebars": "^4.0.12",
